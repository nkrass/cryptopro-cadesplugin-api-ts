{
  "name": "cryptopro-cadesplugin-api-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Modern TypeScript CryptoPro CAdES cadesplugin_api loader (Chromium).",
  "license": "MIT",
  "author": "3BS",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cadesplugin_api": {
      "types": "./dist/cadesplugin_api.d.ts",
      "import": "./dist/cadesplugin_api.js",
      "require": "./dist/cadesplugin_api.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "THIRD_PARTY_NOTICES.md"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "upstream:fetch": "node scripts/fetch-upstream.mjs"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.5.0",
    "typescript": "^5.9.0",
    "vitest": "^4.0.18"
  }
}
